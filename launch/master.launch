<launch>
  <!-- teleop -->
  <include file="$(find solace)/launch/teleop.launch" />

  <!-- navigation stack -->
  <include file="$(find solace)/launch/move_base.launch" />

  <!-- map saver -->
  <node name="map_saver" type="map_saver" pkg="map_server">
    <remap from="map" to="disk_map"/>
  </node>

  <!-- navigation map server -->
  <node name="navigation_map_server" type="navigation_map_server.py" pkg="solace" />

  <!-- lost detector -->
  <node name="lost_detector" type="lost_detector.py" pkg="solace" output="screen" />

  <!-- mapping controller -->
  <node name="mapping_controller" type="mapping_controller.py" pkg="solace" />
</launch>
